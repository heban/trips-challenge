{
  "plugins": ["@typescript-eslint", "@emotion"],
  "extends": ["next/core-web-vitals", "plugin:@typescript-eslint/recommended", "prettier"],
  "env": {
    "browser": true,
    "jasmine": true,
    "jest": true,
    "es6": true
